# Environment Variables for Fitrep Calculator
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (required)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# HuggingFace API Key (optional, for future use)
# Get your key from: https://huggingface.co/settings/tokens
HF_API_TOKEN=your_huggingface_api_key_here

# Enable / Disable local and open source generation options
# These will load in your local environment, not when deployed
ENABLE_LOCAL_OPTION=True
ENABLE_OPEN_WEIGHT_OPTION=True
